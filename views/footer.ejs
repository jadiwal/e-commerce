</div>
<script>
  function checkIdInLocalStorage() {
    return localStorage.getItem('token') !== null;
  }
  function redirectToLogin() {
    window.location.href = '<%=env.url %>login';
  }

  setInterval(() => {
    localStorage.removeItem('token');
  }, 720000)

  let logout = document.getElementById('logout')

  logout.addEventListener('click', (e) => {
    localStorage.removeItem('token');
    window.location.href = '<%=env.url %>login';
  })


  window.onload = function () {
    if (!checkIdInLocalStorage()) {
      redirectToLogin();
    }
  };


  document.querySelector('#mobile_btn').addEventListener("click", function(){
    var sidebar = document.querySelector('.sidebar');
    var currentMarginLeft = parseInt(window.getComputedStyle(sidebar).marginLeft);
    if (currentMarginLeft === 0) {
        sidebar.style.marginLeft = '-575px'; // Change 200px to your desired sidebar width
    } else {
        sidebar.style.marginLeft = '0';
    }
});


// #toggle_btn
</script>

